<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean name="student" class="com.dl.ambiguities.Student">
		<constructor-arg value="201" type="int" name="rollno" index="0"></constructor-arg>
		<constructor-arg value="2000.00" type="double" name="stipend" index="1"></constructor-arg>
		<constructor-arg value="JNTU" type="String" name="university" index="2"></constructor-arg>
	</bean>
	
</beans>